Cell[TextData[{
 StyleBox["Definition ",
  FontWeight->"Bold"],
 StyleBox["(Coset)",
  FontSlant->"Italic"],
 StyleBox[". ",
  FontWeight->"Bold"],
 "If ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "21576ae4-634a-4cc2-bb04-f16cbaaba984"],
 " is a subgroup of ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "adb7e797-a77f-4f34-bf81-88631c2f26e3"],
 ", then for some ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5fe31db2-2d4d-45b4-8bc2-1dc6e1f81f4b"],
 " in ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2e19dc7a-f06f-48d2-8219-46395b980539"],
 ", the set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"a", " ", 
     RowBox[{"h", ":", " ", 
      RowBox[{"h", "\[Element]", "H"}]}]}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "58a57f3a-04ac-4504-9799-73545d8054d7"],
 " is called the left coset of ",
 Cell[BoxData[
  FormBox["H", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9d5dc30d-c60b-4a39-84f2-9ecbe853d80b"],
 " in ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8c1f1b99-1518-4b9e-9979-61f7fce7a7c4"],
 " containing ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "854e33bf-8b83-444d-bab4-43e232b96b72"],
 ". Subsequently, corresponding right coset is the set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"h", " ", 
     RowBox[{"a", ":", " ", 
      RowBox[{"h", "\[Element]", "H"}]}]}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6549dc84-ea06-491f-81ab-0061372574f0"],
 "."
}], "Text",
 CellChangeTimes->{{3.9310623510654526`*^9, 
  3.9310625141591916`*^9}},ExpressionUUID->"b718dedf-9a0a-4c31-bbc9-\
f86a205afd31"],

