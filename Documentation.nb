Cell[TextData[{
 StyleBox["Definition ",
  FontWeight->"Bold"],
 StyleBox["(Group Homomorphism)",
  FontSlant->"Italic"],
 StyleBox[". ",
  FontWeight->"Bold"],
 "A map \[Phi] from ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["G", "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "156bccf4-36d9-47f9-b02f-d9a83697b964"],
 " to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["G", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "041707d0-a6ca-4152-ab2a-141565904312"],
 " is said to be a homomorphism, if and only if it is operation preserving, ",
 StyleBox["i.e.,",
  FontSlant->"Italic"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Phi]", 
      RowBox[{"(", "xy", ")"}]}], "=", 
     RowBox[{"\[Phi]", 
      RowBox[{"(", "x", ")"}], 
      RowBox[{"\[Phi]", "(", "y", ")"}], " ", 
      RowBox[{"\[ForAll]", "x"}]}]}], ",", 
    RowBox[{"y", "\[Element]", 
     SubscriptBox["G", "1"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "665a9fd3-1b44-48c1-b71c-d4c264e2d380"],
 "."
}], "Text",
 CellChangeTimes->{{3.931132104498022*^9, 
  3.931132240489279*^9}},ExpressionUUID->"8e449d1a-6b14-4921-a6cd-\
d5fe56d28f3b"],

